#!/bin/sh

version=${1:-v3}

if [ $version == "v2" ]
then
    echo CONSOLE_STATUS_SCHEDULE="*/3 * * * ? *"
    echo CONSOLE_UPDATE_SCHEDULE="*/10 * * * ? *"
    echo CONSOLE_WEBHOOKS_SCHEDULE="*/5 * * * ? *"
else
    echo CONSOLE_STATUS_SCHEDULE="*/3 * * * *"
    echo CONSOLE_UPDATE_SCHEDULE="*/10 * * * *"
    echo CONSOLE_WEBHOOKS_SCHEDULE="*/5 * * * *"
fi


